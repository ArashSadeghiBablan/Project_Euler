   "source": [
    "#Project Euler Problem 6\n",
    "\n",
    "def ssq(x):\n",
    "    total = 0\n",
    "    for i in range (1,x+1):\n",
    "        total = total + i**2\n",
    "    return total\n",
    "\n",
    "def sqs(x):\n",
    "    total = 0\n",
    "    for i in range (1,x+1):\n",
    "        total = total + i\n",
    "    return total**2\n",
    "\n",
    "def ssd(x):\n",
    "    return sqs(x) - ssq(x)\n",
    "\n",
    "ssd(100)"
   ]
  }
